{
  "name": "plantilla-angular11",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compila-produccion": "ng build --prod --base-href=/Aplicaciones/Dashboard/ --output-path=dist/prod/Dashboard",
    "compila-qa": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=qa --base-href=/Aplicaciones/Dashboard/ --output-path=dist/qa/Dashboard",
    "compila-dev": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=dev --base-href=/Aplicaciones/Dashboard/ --output-path=dist/dev/Dashboard",
    "compila-todos": "npm run compila-produccion && npm run compila-qa && npm run compila-dev"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.13",
    "@angular/cdk": "^11.2.2",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.2.6",
    "@angular/material": "^11.2.2",
    "@angular/material-moment-adapter": "^11.2.5",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "^11.2.9",
    "@angular/router": "~11.2.6",
    "chart.js": "^2.9.4",
    "js2xmlparser": "^4.0.1",
    "moment": "^2.29.1",
    "ng2-charts": "^2.4.2",
    "ngx-toastr": "^13.2.1",
    "rxjs": "~6.6.0",
    "subsink": "^1.0.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.6",
    "@types/chart.js": "^2.9.28",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
